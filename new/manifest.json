{
   "background": {
      "page": "lib/chrome/background.html"
   },
   "description": "Easy Access to YouTube via Sidebar UI",
   "developer": {
      "name": "anthonyfindler"
   },
   "homepage_url": "https://mybrowseraddon.com/youtube-sidebar.html",
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Sidebar for YouTube™",
   "options_ui": {
      "chrome_style": true,
      "open_in_tab": true,
      "page": "data/options/options.html"
   },
   "permissions": [ "storage", "http://*/*", "https://*/*", "webRequest", "webRequestBlocking" ],
   "short_name": "youtube-sidebar",
   "sidebar_action": {
      "default_icon": {
         "16": "data/icons/16.png",
         "32": "data/icons/32.png",
         "48": "data/icons/48.png",
         "64": "data/icons/64.png"
      },
      "default_panel": "data/sidebar/sidebar.html",
      "default_title": "Sidebar for YouTube™"
   },
   "update_url": "https://extension-updates.opera.com/api/omaha/update/",
   "version": "0.1.9",
   "web_accessible_resources": [ "data/sidebar/inject/inject.css" ]
}
{
   "sidebar_action": {
      "default_icon": {
         "128": "data/icons/icon-128.png",
         "32": "data/icons/icon-32.png",
         "38": "data/icons/icon-38.png",
         "512": "data/icons/icon-512.png"
      },
      "default_panel": "data/sidebar/sidebar.html",
      "default_title": "__MSG_app_name__"
   },
   "background": {
      "service_worker": "background.js"
   },
   "host_permissions": [ "\u003Call_urls>" ],
   "default_locale": "en",
